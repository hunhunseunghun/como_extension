{
  "manifest_version": 3,
  "name": "como_turbo",
  "version": "2.0.0",
  "description": "__MSG_extensionDescription__",
  "permissions": ["tabs", "storage", "alarms", "notifications"],
  "host_permissions": ["https://api.upbit.com/*", "https://www.koreaexim.go.kr/*", "https://finance.naver.com/*"],
  "background": {
    "service_worker": "background.iife.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": "como-logo.png"
  },
  "icons": {
    "128": "como-logo.png"
  }
}
